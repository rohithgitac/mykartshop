<section style="margin-top: 1rem;" >
 <div class="container ">
  
  <div class="row mt-1 text-white" >
    <div class="col-md-6">
    <h3 >All users Informations</h3> 
    </div>
  </div>
  <div class="table-responsive">
  <table class="table table-bordered mt-2 ">
    <thead>
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Id</th>
        <th>Address</th>
        <th>Mobile</th>
       
      </tr>
    </thead>
    <tbody class="text-white">
        {{#each usersdata}}
      <tr>
        <td>{{this.name}}</td>
        <td>{{this.email}}</td>
        <td>{{this._id}}</td>
        <td>{{this.address}}</td>
        <td>{{this.mobile}}</td>
      </tr>
      {{/each}}
    </tbody>     
  </table>
  </div>
</div>
</section>